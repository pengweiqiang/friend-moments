<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item  android:state_focused="false" android:state_pressed="false" android:state_selected="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >

            <solid android:color="#f29c9f" />


            <corners
                android:radius="3dp" />



        </shape>

    </item>
    <item  android:state_pressed="false" android:state_selected="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >

            <solid android:color="#ec8d8e" />
            <corners
                android:radius="3dp" />



        </shape>

    </item>
    <!-- Focused states -->
    <item  android:state_focused="true" android:state_pressed="false" android:state_selected="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >

            <solid android:color="#ec8d8e" />


            <corners
                android:radius="3dp" />



        </shape>
    </item>
    <!-- Pressed -->
    <item  android:state_pressed="true">

        <shape xmlns:android="http://schemas.android.com/apk/res/android" >

            <solid android:color="#ec8d8e" />

            <corners
                android:radius="3dp" />


        </shape>

        </item>
</selector>